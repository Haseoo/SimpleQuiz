<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<VBox xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="-1.0" prefWidth="-1.0">
        <children>
          <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Choose category">
            <font>
              <Font size="20.0" />
            </font>
          </Text>
          <HBox alignment="CENTER_LEFT" prefHeight="-1.0" prefWidth="-1.0">
            <children>
              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Choosing player: ">
                <font>
                  <Font size="15.0" />
                </font>
              </Text>
              <Text fx:id="choosingPlayerNickname" strokeType="OUTSIDE" strokeWidth="0.0" text="">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Text>
            </children>
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
          </HBox>
          <VBox fx:id="categoryVBox" prefHeight="-1.0" prefWidth="-1.0">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </VBox>
          <Button fx:id="applyButton" defaultButton="true" mnemonicParsing="false" onAction="#onApply" text="Apply decision">
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </Button>
        </children>
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
      </VBox>
   </children>
</VBox>
