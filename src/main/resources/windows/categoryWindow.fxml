<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="TOP_CENTER" prefHeight="-1.0" prefWidth="-1.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Choose category">
      <font>
        <Font size="20.0" />
      </font>
    </Text>
    <HBox alignment="CENTER_LEFT" prefHeight="-1.0" prefWidth="-1.0">
      <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Choosing player: ">
          <font>
            <Font size="15.0" />
          </font>
        </Text>
        <Text fx:id="choosingPlayerNickname" strokeType="OUTSIDE" strokeWidth="0.0" text="" />
      </children>
    </HBox>
    <VBox fx:id="categoryVBox" prefHeight="-1.0" prefWidth="-1.0" />
    <Button fx:id="applyButton" mnemonicParsing="false" onAction="#onApply" text="Apply decision" />
  </children>
</VBox>
