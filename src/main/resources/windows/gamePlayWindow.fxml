<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="-1.0" prefWidth="-1.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <HBox prefHeight="-1.0" prefWidth="-1.0">
      <children>
        <Label text="Round: ">
               <font>
                  <Font size="20.0" />
               </font></Label>
        <Label fx:id="roundNumber" text="">
               <font>
                  <Font size="20.0" />
               </font></Label>
      </children>
      <VBox.margin>
        <Insets bottom="10.0" />
      </VBox.margin>
    </HBox>
      <StackPane fx:id="contentPane" alignment="TOP_CENTER" />
      <Button fx:id="nextButton" alignment="CENTER" defaultButton="true" disable="true" maxWidth="250.0" mnemonicParsing="false" onAction="#onNext" text="Next" textAlignment="CENTER">
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </Button>
  </children>
</VBox>
